<?xml version="1.0" encoding="UTF-8"?>
<endpoint name="chambreEP_supprimer" xmlns="http://ws.apache.org/ns/synapse">
    <http method="delete" uri-template="http://localhost:3000/chambres/:numeroChambre">
        <suspendOnFailure>
            <initialDuration>-1</initialDuration>
            <progressionFactor>1.0</progressionFactor>
        </suspendOnFailure>
        <markForSuspension>
            <retriesBeforeSuspension>0</retriesBeforeSuspension>
        </markForSuspension>
    </http>
</endpoint>
